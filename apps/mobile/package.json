{
  "name": "@sparecarry/mobile",
  "version": "0.1.0",
  "private": true,
  "main": "./index.js",
  "scripts": {
    "dev": "expo start --clear",
    "start": "expo start --clear",
    "start:clear": "expo start --clear",
    "start:offline": "expo start --offline",
    "start:tunnel": "expo start --tunnel --clear",
    "start:lan": "expo start --lan --clear",
    "start:no-check": "EXPO_NO_DOTENV=1 expo start --no-dev-client",
    "android": "npx @expo/cli@latest run:android",
    "android:device": "npx @expo/cli@latest run:android --device",
    "android:apk": "npx @expo/cli@latest run:android --variant release",
    "ios": "npx @expo/cli@latest run:ios",
    "build:dev": "eas build --platform android --profile development",
    "build:preview": "eas build --platform android --profile preview",
    "build:prod": "eas build --platform android --profile production",
    "web": "expo start --web",
    "prebuild": "npx @expo/cli@latest prebuild",
    "prebuild:clean": "npx @expo/cli@latest prebuild --clean",
    "clear-cache": "node scripts/clear-cache.js || powershell -ExecutionPolicy Bypass -File scripts/clear-cache.ps1",
    "sync-env": "node scripts/sync-env-from-root.js",
    "get-lan-ip": "node scripts/get-lan-ip.js",
    "switch-to-npm": "node scripts/switch-to-npm.js",
    "verify-react": "node scripts/verify-react-version.js",
    "typecheck": "tsc --noEmit",
    "test:continuous": "cd ../.. && pnpm test:continuous"
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/native": "^7.1.8",
    "@sparecarry/hooks": "workspace:*",
    "@sparecarry/lib": "workspace:*",
    "@sparecarry/ui": "workspace:*",
    "@supabase/supabase-js": "^2.84.0",
    "@tanstack/react-query": "^5.17.0",
    "date-fns": "^4.1.0",
    "expo": "~54.0.25",
    "expo-build-properties": "^1.0.9",
    "expo-camera": "~17.0.9",
    "expo-clipboard": "^8.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-device": "~8.0.9",
    "expo-file-system": "~19.0.19",
    "expo-font": "~14.0.9",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.9",
    "expo-localization": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.13",
    "expo-router": "~6.0.15",
    "expo-secure-store": "~15.0.7",
    "expo-sensors": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.0",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-view-shot": "^4.0.3",
    "react-native-vision-camera": "^4.0.0",
    "react-native-vision-camera-mlkit": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "typescript": "^5.3.0"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "expo": {
    "install": {
      "exclude": [
        "react",
        "react-dom",
        "react-native",
        "@types/react",
        "@types/react-dom"
      ]
    }
  }
}
