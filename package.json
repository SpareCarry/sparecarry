{
  "name": "sparecarry",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "pnpm dev:web",
    "dev:web": "cross-env NEXT_DISABLE_SWC_NATIVE=1 NEXT_DISABLE_AUTO_MOD_IMPORTS=1 NODE_ENV=development next dev",
    "dev:mobile": "cd apps/mobile && expo start --dev-client",
    "build:dev:android": "cd apps/mobile && eas build --platform android --profile development",
    "build:dev:ios": "cd apps/mobile && eas build --platform ios --profile development",
    "dev:webpack": "cross-env NEXT_DISABLE_SWC_NATIVE=1 NEXT_DISABLE_AUTO_MOD_IMPORTS=1 pnpm dev -- --no-turbopack",
    "prebuild": "node scripts/pre-build-exclude-routes.js",
    "build": "pnpm build:web",
    "build:web": "cross-env NEXT_DISABLE_SWC_NATIVE=1 NEXT_DISABLE_AUTO_MOD_IMPORTS=1 next build",
    "build:mobile": "cd apps/mobile && eas build --platform all",
    "postbuild": "node scripts/fix-aliases.js && node scripts/pre-build-exclude-routes.js restore",
    "validate:export": "node scripts/validate-export.js",
    "mobile:sync": "echo 'Use Expo instead: cd apps/mobile && pnpm start'",
    "mobile:build": "cd apps/mobile && eas build --platform all",
    "mobile:ios": "cd apps/mobile && eas build --platform ios",
    "mobile:android": "cd apps/mobile && eas build --platform android",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "capacitor:sync": "echo 'Capacitor removed - use Expo: cd apps/mobile && pnpm start'",
    "capacitor:ios": "echo 'Capacitor removed - use Expo: cd apps/mobile && pnpm ios'",
    "capacitor:android": "echo 'Capacitor removed - use Expo: cd apps/mobile && pnpm android'",
    "capacitor:build": "echo 'Capacitor removed - use Expo: cd apps/mobile && eas build'",
    "mobile:setup": "pnpm mobile:build",
    "generate:icons": "node scripts/generate-icons.js",
    "generate:splash": "node scripts/generate-splash.js",
    "generate:assets": "pnpm generate:icons && pnpm generate:splash",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:ui": "cross-env NODE_ENV=test vitest --ui",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:with-report": "node scripts/run-vitest-with-report.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:auth": "playwright test tests/e2e/auth-flow.spec.ts",
    "test:e2e:subscription": "playwright test tests/e2e/subscription-flow.spec.ts",
    "test:e2e:with-report": "node scripts/run-playwright-with-report.js",
    "test:e2e:stable": "node scripts/test-e2e-stable.js",
    "test:all": "pnpm test && pnpm test:e2e",
    "test:all:with-reports": "node scripts/run-all-tests-with-reports.js",
    "test:auto-fix": "node scripts/auto-test-runner.js",
    "test:beta": "node scripts/run-beta-tests.js",
    "test:coverage-report": "node scripts/generate-test-coverage-report.js",
    "test:verify-readiness": "node scripts/verify-beta-readiness.js",
    "playwright:install": "playwright install",
    "test:features": "node scripts/test-all-features.js",
    "test:comprehensive": "node scripts/test-comprehensive.js",
    "test:payment-flow": "powershell -ExecutionPolicy Bypass -File scripts/test-payment-flow.ps1",
    "test:e2e:docker": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "coverage": "vitest run --coverage",
    "typecheck": "pnpm typecheck:web && pnpm typecheck:tests && pnpm typecheck:mobile",
    "typecheck:web": "tsc --noEmit -p tsconfig.json",
    "typecheck:app": "tsc --noEmit -p tsconfig.json",
    "typecheck:tests": "tsc --noEmit --project tests/tsconfig.json",
    "typecheck:mobile": "pnpm --filter @sparecarry/mobile typecheck",
    "typecheck:coverage": "type-coverage --detail",
    "typecheck:prune": "ts-prune",
    "typecheck:all": "pnpm typecheck && pnpm typecheck:coverage && pnpm typecheck:prune",
    "e2e:android": "detox test --configuration android.emu.debug",
    "e2e:ios": "detox test --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:android:ci": "detox test --configuration android.emu.debug --headless",
    "e2e:ios:ci": "detox test --configuration ios.sim.debug --headless",
    "preflight": "node scripts/preflight-check.js",
    "preflight:beta": "node scripts/preflight-beta.js",
    "validate:env": "node scripts/validate-env.js",
    "verify:mobile": "node scripts/verify-mobile-build.js",
    "db:migrate:staging": "node scripts/migrate-staging-db.js",
    "db:seed:staging": "node scripts/seed-staging-data.js",
    "db:rollback:staging": "bash scripts/rollback-staging-db.sh",
    "db:setup": "node scripts/setup-supabase.js",
    "db:setup:bash": "bash scripts/setup-supabase.sh",
    "db:apply": "node scripts/apply-migrations-direct.js",
    "qa:run": "node scripts/final_qa_script.js",
    "version:bump": "node scripts/version-bump.js",
    "release:notes": "node scripts/release-notes.js",
    "env:sync": "node scripts/sync-env-build.js",
    "loadtest": "cd load-tests && k6 run scenarios/steady.js",
    "build:staging": "cross-env NEXT_DISABLE_SWC_NATIVE=1 NEXT_DISABLE_AUTO_MOD_IMPORTS=1 NEXT_PUBLIC_APP_ENV=staging next build",
    "build:production": "cross-env NEXT_DISABLE_SWC_NATIVE=1 NEXT_DISABLE_AUTO_MOD_IMPORTS=1 NEXT_PUBLIC_APP_ENV=production next build",
    "mobile:build:staging": "cd apps/mobile && cross-env NEXT_PUBLIC_APP_ENV=staging eas build --platform all --profile staging",
    "mobile:build:production": "cd apps/mobile && cross-env NEXT_PUBLIC_APP_ENV=production eas build --platform all --profile production",
    "eas:build:android": "cd apps/mobile && eas build --platform android",
    "eas:build:ios": "cd apps/mobile && eas build --platform ios",
    "eas:build:all": "cd apps/mobile && eas build --platform all"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-google-maps/api": "^2.19.3",
    "@sentry/nextjs": "^7.114.0",
    "@stripe/react-stripe-js": "2.9.0",
    "@stripe/stripe-js": "2.4.0",
    "@supabase/ssr": "0.7.0",
    "@supabase/supabase-js": "2.84.0",
    "@tanstack/react-query": "^5.17.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "dotenv": "^17.2.3",
    "eas-cli": "^16.28.0",
    "lucide-react": "0.309.0",
    "nanoid": "3.3.7",
    "next": "14.2.5",
    "next-intl": "^3.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.49.3",
    "resend": "^3.2.0",
    "stripe": "^14.10.0",
    "tailwind-merge": "^2.2.0",
    "twilio": "^4.20.0",
    "web-streams-polyfill": "^3.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@playwright/test": "^1.40.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/google.maps": "^3.55.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.22",
    "cross-env": "^7.0.3",
    "detox": "^20.14.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "expo-device": "~8.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.13",
    "glob": "7.2.3",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-native": "0.76.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "ts-prune": "^0.10.3",
    "type-coverage": "^2.28.0",
    "typescript": "5.9.3",
    "typescript-json-schema": "^0.55.0",
    "vitest": "^1.0.4"
  },
  "pnpm": {
    "overrides": {
      "nanoid": "3.3.7",
      "@swc/core": "1.3.71",
      "lucide-react": "0.309.0",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-native": "0.81.5",
      "@types/react": "~19.1.10",
      "@types/react-dom": "~19.1.7",
      "@sparecarry/mobile>react": "19.1.0",
      "@sparecarry/mobile>react-dom": "19.1.0",
      "glob": "7.2.3",
      "rimraf": "^5.0.0",
      "@babel/plugin-proposal-class-properties": "npm:@babel/plugin-transform-class-properties@^7.25.0",
      "@babel/plugin-proposal-nullish-coalescing-operator": "npm:@babel/plugin-transform-nullish-coalescing-operator@^7.25.0",
      "@babel/plugin-proposal-optional-chaining": "npm:@babel/plugin-transform-optional-chaining@^7.25.0",
      "@humanwhocodes/config-array": "npm:@eslint/config-array@^0.18.0",
      "@humanwhocodes/object-schema": "npm:@eslint/object-schema@^2.1.0",
      "inflight": "npm:once@^1.4.0"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "19.1.0"
      }
    },
    "ignoredBuiltDependencies": [
      "@sentry/cli",
      "detox",
      "dtrace-provider",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "resolutions": {
    "nanoid": "3.3.7",
    "glob": "7.2.3",
    "rimraf": "^5.0.0",
    "inflight": "npm:once@^1.4.0"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "nanoid": "3.3.7"
  },
  "engines": {
    "node": ">=22 <25",
    "pnpm": ">=8.0.0"
  }
}