#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/SpareCarry/node_modules/.pnpm/react-native@0.76.0_@babel+_696b1cd0a59f13048f412c8bf0c93d74/node_modules/react-native/node_modules:/mnt/c/SpareCarry/node_modules/.pnpm/react-native@0.76.0_@babel+_696b1cd0a59f13048f412c8bf0c93d74/node_modules:/mnt/c/SpareCarry/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/SpareCarry/node_modules/.pnpm/react-native@0.76.0_@babel+_696b1cd0a59f13048f412c8bf0c93d74/node_modules/react-native/node_modules:/mnt/c/SpareCarry/node_modules/.pnpm/react-native@0.76.0_@babel+_696b1cd0a59f13048f412c8bf0c93d74/node_modules:/mnt/c/SpareCarry/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-native/cli.js" "$@"
else
  exec node  "$basedir/../react-native/cli.js" "$@"
fi
